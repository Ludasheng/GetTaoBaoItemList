def scan_code_login(self):
    """
    扫码登录
    :return:
    """
    url = "https://www.taobao.com"
    self.browser.get(url)
    self.browser.maximize_window()

    if self.browser.find_element_by_link_text("亲，请登录"):
        self.browser.find_element_by_link_text("亲，请登录").click()

    time.sleep(2)
    if self.browser.find_element_by_id("J_QRCodeImg"):
        print('get the QRCodeImgUrl.....')
        print(self.browser.find_element_by_id("J_QRCodeImg").find_element_by_tag_name("img").get_attribute("src"))
        self.browser.find_element_by_id("J_QRCodeImg").find_element_by_tag_name("img").get_attribute("src")

    while True:
        try:
            if self.browser.find_element_by_link_text("密码登录"):
                print("请扫码登录...")
                time.sleep(2)
        except:
            print("成功登录...")
            print(self.browser.current_url)
            break
